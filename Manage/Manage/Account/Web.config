<configuration>
	<location path="Register.aspx">
		<system.web>
			<authorization>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<location path="RecoverPassword.aspx">
		<system.web>
			<authorization>
				<allow users="?"/>
			</authorization>
		</system.web>
	</location>
	<system.web>
		<authorization>
			<deny users="?"/>
		</authorization>
	</system.web>
</configuration>